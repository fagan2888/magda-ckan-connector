{
    "extends": "./tsconfig-build.json",
    "compilerOptions": {
        "target": "es5",
        "baseUrl": ".",
        "paths": {
            "@magda/registry-aspects/dist/*": [
                "./node_modules/@magda/registry-aspects/src/*"
            ],
            "@magda/typescript-common/dist/*": [
                "./node_modules/@magda/typescript-common/src/*"
            ]
        },
        "lib": [
            "dom",
            "es5",
            "scripthost",
            "es2015.promise"
        ]
    },
    "exclude": [
        "src/index.ts"
    ]
}
